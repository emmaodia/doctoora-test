<a href="/">Home</a>
<% if user_signed_in? %>
	<div>
		<li><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></li>
		<li><%= link_to "Patient Profile", profile_path(current_user) %></li>
	</div>
<% else %>
	<div>
		<li><%= link_to "Sign Up As A Member", new_user_registration_path %></li>
		<li><%= link_to "Log In", new_user_session_path %></li>
	</div>
<% end %>