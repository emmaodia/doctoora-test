<div>
	<%= link_to "admin home", admin_path, :class => "admin-home" %>
</div>

<div class="row">
	<% @clinic_rentals.each do |rental| %>
		<div class="panel panel-body col-md-12">
			<h2> <%= get_clinic_name rental.clinic_id %> </h2>
			<p> Rented by: <a href="/doctor_profile/<%= rental.doctor_id %>"><%= get_doctor_name rental.doctor_id %></a></p>
			<p> Date: <%= rental.date.strftime("%m/%d/%y") %> </p>
			<p> Time: <%= rental.time.strftime("%H:%M") %></p>
		</div>
	<% end %>
</div>

<style>
  #activity-wrapper {
    display: none;
  }
</style>