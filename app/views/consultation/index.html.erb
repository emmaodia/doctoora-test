<%= render 'shared/nav' %>

<h1> Your Consultations </h1>

<div>
	<% @consultations.each do |consultation| %>
		<div>
			<h4>Consultation date</h4> <%= consultation.date %>
			<h4>Consultation time</h4> <%= consultation.start_time %>
			<h4>Professional</h4> <%= consultation.professional %>
			<h4>Tool</h4> <%= consultation.tool %>
		</div>
	<% end %>
</div>
	